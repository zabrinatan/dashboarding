<h1>Dashboard#index</h1>
<p>Find me in app/views/dashboard/index.html.erb</p>


<% @dashboards.each do |dashboard| %>
<p><%= link_to "#{dashboard.name}" , dashboard_path(dashboard.id) %></p>
<div class="controls">
  <%= button_to "Delete #{dashboard.name}", dashboard_path(dashboard.id), :method => 'delete' %>
</div>
<% end %>
